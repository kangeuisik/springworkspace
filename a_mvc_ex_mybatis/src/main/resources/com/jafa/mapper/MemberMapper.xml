<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jafa.mapper.MemberMapper"><!-- 인터페이스경로 꼭같아야함-->
  <select id="findByMno" resultType="member"> <!-- 메소드이름, 알리아스 지정 -->
    select * from Member_mybatis where mno = #{mno}
  </select>
</mapper>